{
    "name": "prestashop-products-api",
    "type": "project",
    "description": "API Laravel para obtener productos de PrestaShop",
    "keywords": ["laravel", "prestashop", "api"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^10.0",
        "illuminate/http": "^10.0",
        "illuminate/database": "^10.0",
        "illuminate/routing": "^10.0",
        "vlucas/phpdotenv": "^5.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

